\n                <h4 class=\"text-lg text-gray-100 font-semibold mb-6\">\n                  CAMPAIGNS\n                </h4>\n                <div class=\"w-full mt-6 pb-6 overflow-x-auto\">\n                  <table class=\"w-full min-w-max\">\n                    <thead>\n                      <tr class=\"text-left\">\n                        <th class=\"p-0\">\n                          <div class=\"py-3 px-6 rounded-l-xl bg-red-600\">\n                            <span class=\"text-xs text-gray-300 font-semibold\"\n                              >CAMPAIGN NAME</span\n                            >\n                          </div>\n                        </th>\n                        <th class=\"p-0\">\n                          <div class=\"py-3 px-6 bg-red-600\">\n                            <span class=\"text-xs text-gray-300 font-semibold\"\n                              >KEYWORD</span\n                            >\n                          </div>\n                        </th>\n                        <th class=\"p-0\">\n                          <div class=\"py-3 px-6 bg-red-600\">\n                            <span class=\"text-xs text-gray-300 font-semibold\"\n                              >STATUS</span\n                            >\n                          </div>\n                        </th>\n                        <th class=\"p-0\">\n                          <div class=\"py-3 px-6 bg-red-600\">\n                            <span class=\"text-xs text-gray-300 font-semibold\"\n                              >START TIME</span\n                            >\n                          </div>\n                        </th>\n                        <th class=\"p-0\">\n                          <div class=\"py-3 px-6 bg-red-600\">\n                            <span class=\"text-xs text-gray-300 font-semibold\"\n                              >END TIME</span\n                            >\n                          </div>\n                        </th>\n                        <th class=\"p-0\">\n                          <div class=\"py-6 px-6 bg-red-600\"></div>\n                        </th>\n                        <th class=\"p-0\">\n                          <div class=\"py-6 px-6 bg-red-600\">\n                            <span class=\"text-xs text-gray-300 font-semibold\">\n                            </span>\n                          </div>\n                        </th>\n                        <th class=\"p-0\">\n                          <div class=\"py-6 px-6 bg-red-600 rounded-r-xl\">\n                            <span class=\"text-xs text-gray-300 font-semibold\">\n                            </span>\n                          </div>\n                        </th>\n                      </tr>\n                    </thead>\n                    <!-- start table records -->\n                    <tbody>\n                      <tr\n                        v-for=\"campaign in campaigns\"\n                        v-bind:key=\"campaign.campaign_id\"\n                      >\n                        <td class=\"p-0\">\n                          <div class=\"flex items-center h-16 px-6\">\n                            <span class=\"text-sm text-gray-100 font-medium\">{{\n                              campaign.campaign_name\n                            }}</span>\n                          </div>\n                        </td>\n                        <td class=\"p-0\">\n                          <div class=\"flex items-center h-16 px-6\">\n                            <span class=\"text-sm text-gray-100 font-medium\"\n                              >{{ campaign.keyword }}\n                            </span>\n                          </div>\n                        </td>\n                        <td class=\"p-0\">\n                          <div class=\"flex items-center h-16 px-6\">\n                            <span\n                              class=\"text-sm font-medium\"\n                              v-bind:class=\"statusColor(campaign.status)\"\n                            >\n                              {{ statusConvert(campaign.status) }}\n                            </span>\n                          </div>\n                        </td>\n                        <td class=\"p-0\">\n                          <div class=\"flex items-center h-16 px-6\">\n                            <div class=\"flex h-full items-center\">\n                              <span class=\"text-sm font-medium text-gray-100\">{{\n                                campaign.start_time\n                              }}</span>\n                            </div>\n                          </div>\n                        </td>\n                        <td class=\"p-0\">\n                          <div class=\"flex items-center h-16 px-6\">\n                            <span class=\"text-sm text-gray-100 font-medium\">{{\n                              campaign.end_time\n                            }}</span>\n                          </div>\n                        </td>\n                        <td class=\"p-0\">\n                          <button\n                            v-bind:disabled=\"\n                              campaign.status == 2 || campaign.status == 3\n                            \"\n                          >\n                            <div v-if=\"campaign.status == 1\">\n                              <router-link\n                                :to=\"{\n                                  name: 'adminModifyCampaign',\n                                  params: { id: campaign.campaign_id },\n                                }\"\n                              >\n                                <a\n                                  class=\"inline-block py-2 px-4 text-xs text-center font-semibold leading-normal text-blue-50 bg-blue-500 hover:bg-blue-600 rounded-lg transition duration-200\"\n                                  >編集</a\n                                >\n                              </router-link>\n                            </div>\n                            <div v-else>\n                              <a\n                                class=\"inline-block py-2 px-4 text-xs text-center font-semibold leading-normal text-blue-50 bg-gray-700 hover:bg-gray-800 rounded-lg transition duration-200\"\n                                >編集</a\n                              >\n                            </div>\n                          </button>\n                        </td>\n                        <td class=\"p-0\">\n                          <button>\n                            <router-link\n                              :to=\"{\n                                name: 'AdminAddOptions',\n                                params: { id: campaign.campaign_id },\n                              }\"\n                            >\n                              <a\n                                class=\"inline-block py-2 px-4 text-xs text-center font-semibold leading-normal text-blue-50 bg-blue-500 hover:bg-blue-600 rounded-lg transition duration-200\"\n                                href=\"#\"\n                                >追加設定</a\n                              >\n                            </router-link>\n                          </button>\n                        </td>\n                        <td class=\"p-0\">\n                          <button>\n                            <router-link\n                              :to=\"{\n                                name: 'adminAnalisys',\n                                params: { id: campaign.campaign_id },\n                              }\"\n                            >\n                              <a\n                                class=\"inline-block py-2 px-4 text-xs text-center font-semibold leading-normal text-blue-50 bg-blue-500 hover:bg-blue-600 rounded-lg transition duration-200\"\n                                href=\"#\"\n                                >分析</a\n                              >\n                            </router-link>\n                          </button>\n                        </td>\n                      </tr>\n                    </tbody>\n                  </table>\n                </div>\n              </div>\n              <div\n                class=\"flex flex-wrap items-center justify-between px-6 pt-8 border-t border-gray-400\"\n              >\n                <div class=\"w-full sm:w-auto mb-6 sm:mb-0\">\n                  <div class=\"flex items-center justify-between\">\n                    <p class=\"text-sm font-medium text-gray-400\">\n                      <span>Showing</span>\n                      <span class=\"px-px text-gray-200\">1</span>\n                      <span>to</span>\n                      <span class=\"px-px text-gray-200\">1</span>\n                      <span>of 1 results</span>\n                    </p>\n                  </div>\n                </div>\n                <div class=\"w-full sm:w-auto mb-6 sm:mb-0\">\n                  <div class=\"flex flex-wrap items-center -m-1.5\">\n                    <div class=\"w-auto p-1.5\">\n                      <a\n                        class=\"inline-flex items-center h-9 py-1 px-4 text-xs text-gray-400 font-semibold bg-gray-600 hover:bg-gray-700 rounded-lg transition duration-200\"\n                        href=\"#\"\n                        >Previous</a\n                      >\n                    </div>\n                    <div class=\"w-auto p-1.5\">\n                      <a\n                        class=\"inline-flex items-center h-9 py-1 px-4 text-xs text-blue-50 font-semibold bg-gray-500 hover:bg-gray-600 rounded-lg transition duration-200\"\n                        href=\"#\"\n                        >Next</a\n                      >\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </section>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport AdminSideVar from \"@/components/AdminSideVar.vue\";\nimport adminapi from \"../api/adminapi.js\";\nimport moment from \"moment\";\n\nexport default {\n  name: \"Campaign_list\",\n  components: {\n    AdminSideVar,\n  },\n  data() {\n    return {\n      campaigns: null,\n    };\n  },\n  mounted() {\n    this.getCampaigns();\n  },\n  methods: {\n    getCampaigns() {\n      adminapi\n        .getCampaigns()\n        .then((response) => {\n          this.campaigns = response.data;\n          for (var i in this.campaigns) {\n            let start_date = new Date(this.campaigns[i][\"start_time\"]);\n            start_date.setHours(start_date.getHours() + 9); // 日本時間に変更\n            let end_date = new Date(this.campaigns[i][\"end_time\"]);\n            end_date.setHours(end_date.getHours() + 9); // 日本時間に変更\n            this.campaigns[i][\"start_time\"] = moment(start_date).format(\n              \"YYYY-MM-DD HH:mm:SS\"\n            );\n            this.campaigns[i][\"end_time\"] = moment(end_date).format(\n              \"YYYY-MM-DD HH:mm:SS\"\n            );\n          }\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n    statusConvert(status) {\n      switch (status) {\n        case 1:\n          return \"開始待ち\";\n        case 2:\n          return \"開催中\";\n        case 3:\n          return \"終了\";\n      }\n    },\n    statusColor(status) {\n      switch (status) {\n        case 1:\n          return \"text-yellow-500\";\n        case 2:\n          return \"text-green-500\";\n        case 3:\n          return \"text-gray-300\";\n      }\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}