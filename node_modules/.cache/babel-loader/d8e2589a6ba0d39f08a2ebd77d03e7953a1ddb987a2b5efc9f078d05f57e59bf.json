{"ast":null,"code":"import \"core-js/modules/es.date.to-string.js\";\nimport AdminSideVar from \"@/components/AdminSideVar.vue\";\nimport adminapi from \"../api/adminapi.js\";\nimport moment from \"moment\";\nexport default {\n  name: \"Campaign_list\",\n  components: {\n    AdminSideVar: AdminSideVar\n  },\n  data: function data() {\n    return {\n      campaigns: null\n    };\n  },\n  mounted: function mounted() {\n    this.getCampaigns();\n  },\n  methods: {\n    getCampaigns: function getCampaigns() {\n      var _this = this;\n      adminapi.getCampaigns().then(function (response) {\n        _this.campaigns = response.data;\n        for (var i in _this.campaigns) {\n          var start_date = new Date(_this.campaigns[i][\"start_time\"]);\n          start_date.setHours(start_date.getHours() + 9); // 日本時間に変更\n          var end_date = new Date(_this.campaigns[i][\"end_time\"]);\n          end_date.setHours(end_date.getHours() + 9); // 日本時間に変更\n          _this.campaigns[i][\"start_time\"] = moment(start_date).format(\"YYYY-MM-DD HH:mm:SS\");\n          _this.campaigns[i][\"end_time\"] = moment(end_date).format(\"YYYY-MM-DD HH:mm:SS\");\n        }\n      })[\"catch\"](function (error) {\n        console.error(error);\n      });\n    },\n    statusConvert: function statusConvert(status) {\n      switch (status) {\n        case 1:\n          return \"開始待ち\";\n        case 2:\n          return \"開催中\";\n        case 3:\n          return \"終了\";\n      }\n    },\n    statusColor: function statusColor(status) {\n      switch (status) {\n        case 1:\n          return \"text-yellow-500\";\n        case 2:\n          return \"text-green-500\";\n        case 3:\n          return \"text-gray-300\";\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAgOA,OAAOA,YAAW,MAAO,+BAA+B;AACxD,OAAOC,QAAO,MAAO,oBAAoB;AACzC,OAAOC,MAAK,MAAO,QAAQ;AAE3B,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVJ,YAAY,EAAZA;EACF,CAAC;EACDK,IAAI,kBAAG;IACL,OAAO;MACLC,SAAS,EAAE;IACb,CAAC;EACH,CA