{"ast":null,"code":"import SideVar from \"@/components/SideVar.vue\";\n// import api from \"../api/api.js\";\n// import { ref } from \"vue\";\n// import { useRoute } from \"vue-router\";\n\nexport default {\n  name: \"externalConnect\",\n  components: {\n    SideVar: SideVar\n  },\n  data: function data() {\n    return {};\n  },\n  mounted: function mounted() {},\n  computed: {},\n  watch: {},\n  methods: {}\n};","map":{"version":3,"mappings":"AA+HA,OAAOA,OAAM,MAAO,0BAA0B;AAC9C;AACA;AACA;;AAEA,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVF,OAAO,EAAPA;EACF,CAAC;EACDG,IAAI,kBAAG;IACL,OAAO,CAAC,CAAC;EACX,CAAC;EACDC,OAAO,qBAAG,CAAC,CAAC;EACZC,QAAQ,EAAE,CAAC,CAAC;EACZC,KAAK,EAAE,CAAC,CAAC;EACTC,OAAO,EAAE,CAAC;AACZ,CAAC","names":["SideVar","name","components","data","mounted","computed","watch","methods"],"sourceRoot":"","sources":["/Users/kazuaki.aihara/Documents/fuelcrypto-organizer/src/views/ExternalConnect.vue"],"sourcesContent":["<template>\n  <div class=\"campaignAddConditions\">\n    <!-- side var  -->\n    <SideVar />\n    <!-- input panel -->\n    <div class=\"mx-auto lg:ml-80\">\n      <section class=\"py-3\">\n        <div class=\"container px-4 mx-auto\">\n          <div class=\"p-8 bg-gray-500 rounded-xl\">\n            <div\n              class=\"flex flex-wrap items-center justify-between -mx-4 mb-8 pb-6 border-b border-gray-400 border-opacity-20\"\n            >\n              <div class=\"w-full sm:w-auto px-4 mb-6 sm:mb-0\">\n                <h2\n                  class=\"mb-6 md:text-3xl font-bold font-heading tracking-px-n leading-none md:max-w-xl mx-auto text-gray-100\"\n                >\n                  External Connect\n                </h2>\n              </div>\n              <div class=\"w-full sm:w-auto px-4\"></div>\n            </div>\n            <p class=\"flex justify-left items-center\">\n              <img\n                class=\"w-8 mx-4\"\n                src=\"https://sociama-product-image.s3.ap-northeast-1.amazonaws.com/iconfinder-social-media-applications-6twitter-4102580_113802.png\"\n                alt=\"twitter_icon\"\n              />\n              Twitter：{{ user[\"displayName\"] }}\n            </p>\n            <div class=\"flex justify-left items-center my-4\">\n              <img\n                class=\"w-8 mx-4\"\n                src=\"https://sociama-product-image.s3.ap-northeast-1.amazonaws.com/discord_icon_130958.png\"\n                alt=\"discord_icon\"\n              />\n              Discord：\n              <div\n                v-if=\"discord_user\"\n                class=\"flex justify-left items-center my-4\"\n              >\n                <img\n                  :src=\"\n                    'https://cdn.discordapp.com/avatars/' +\n                    discord_user.user_id +\n                    '/' +\n                    discord_user.avatar\n                  \"\n                  alt=\"discord_icon\"\n                  class=\"w-8 mx-2\"\n                />\n                <p></p>\n                {{ discord_user.username }}\n                <button\n                  class=\"text-sm lg:text-lg py-1 px-3 mx-2 w-full text-white font-semibold rounded-xl focus:ring focus:ring-gray-300 transition ease-in-out duration-200\"\n                  :class=\"\n                    discord_disconnect_loading\n                      ? 'bg-gray-800 hover:bg-gray-900'\n                      : 'bg-gray-600 hover:bg-gray-700'\n                  \"\n                  type=\"button\"\n                  @click=\"disconnect_discord\"\n                >\n                  <i\n                    class=\"pi pi-spin pi-spinner text-sm mx-1\"\n                    v-if=\"discord_disconnect_loading\"\n                  ></i\n                  >Disconnect\n                </button>\n              </div>\n              <div v-else>\n                <a\n                  href=\"https://discord.com/api/oauth2/authorize?client_id=1120705399532826754&redirect_uri=http%3A%2F%2Flocalhost%3A8080%2Faccount&response_type=code&scope=identify\"\n                  ><button\n                    class=\"text-sm lg:text-lg py-1 px-3 w-full text-white font-semibold rounded-xl focus:ring focus:ring-blue-300 transition ease-in-out duration-200\"\n                    :class=\"\n                      discord_loading\n                        ? 'bg-blue-800 hover:bg-blue-900'\n                        : 'bg-blue-600 hover:bg-blue-700'\n                    \"\n                    type=\"button\"\n                    :disabled=\"discord_loading\"\n                  >\n                    <i\n                      class=\"pi pi-spin pi-spinner text-sm mx-1\"\n                      v-if=\"discord_loading\"\n                    ></i\n                    >Connect\n                  </button></a\n                >\n              </div>\n            </div>\n            <div class=\"flex justify-left items-center my-4\">\n              <!-- <i class=\"w-8 text-lg mx-4 pi pi-wallet\" /> -->\n              <img\n                class=\"w-8 mx-4\"\n                src=\"https://sociama-product-image.s3.ap-northeast-1.amazonaws.com/MetaMask_Fox.svg.png\"\n                alt=\"metamask_icon\"\n              />\n              Wallet：\n              <div v-if=\"wallet\" class=\"flex items-center\">\n                {{ wallet }}\n                <button\n                  class=\"text-sm lg:text-lg py-1 px-3 w-full text-white font-semibold rounded-xl focus:ring focus:ring-gray-300 bg-gray-600 hover:bg-gray-700 transition ease-in-out duration-200 mx-3\"\n                  type=\"button\"\n                  @click=\"disconnect_wallet\"\n                >\n                  Disconnect\n                </button>\n              </div>\n              <div v-else class=\"flex\">\n                <button\n                  class=\"text-sm lg:text-lg py-1 px-3 w-auto text-white font-semibold rounded-xl focus:ring focus:ring-blue-300 bg-blue-600 hover:bg-blue-700 transition ease-in-out duration-200 mx-3\"\n                  type=\"button\"\n                  @click=\"connect_wallet\"\n                >\n                  Connect Metamask\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n      </section>\n    </div>\n  </div>\n</template>\n\n<script>\nimport SideVar from \"@/components/SideVar.vue\";\n// import api from \"../api/api.js\";\n// import { ref } from \"vue\";\n// import { useRoute } from \"vue-router\";\n\nexport default {\n  name: \"externalConnect\",\n  components: {\n    SideVar,\n  },\n  data() {\n    return {};\n  },\n  mounted() {},\n  computed: {},\n  watch: {},\n  methods: {},\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}