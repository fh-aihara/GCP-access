   for (var i in _conditions) {\n          var campaign_id = _conditions[i].campaign_id;\n          if (_conditions[i].actions == \"FOLLOW\") {\n            if (_this2.conditions_dict[campaign_id][0].length == 0) {\n              _this2.conditions_dict[campaign_id][0] = _conditions[i].user_id;\n            }\n          }\n          if (_conditions[i].actions == \"RETWEET\") {\n            if (_this2.conditions_dict[campaign_id][1].length == 0) {\n              _this2.conditions_dict[campaign_id][1] = _conditions[i].tweet_id;\n            }\n          }\n        }\n      })[\"catch\"](function (error) {\n        console.error(error);\n      });\n    },\n    statusConvert: function statusConvert(status) {\n      switch (status) {\n        case 1:\n          return \"開始待ち\";\n        case 2:\n          return \"開催中\";\n        case 3:\n          return \"終了\";\n      }\n    },\n    statusColor: function statusColor(status) {\n      switch (status) {\n        case 1:\n          return \"text-yellow-500\";\n        case 2:\n          return \"text-green-500\";\n        case 3:\n          return \"text-gray-300\";\n      }\n    },\n    view_switch_change: function view_switch_change(target) {\n      this.view_switch = target;\n    }\n  }\n};","map":{"version":3,"mappings":";;AAkcA,OAAOA,OAAM,MAAO,0BAA0B;AAC9C,OAAOC,GAAE,MAAO,eAAe;AAC/B,OAAOC,MAAK,MAAO,QAAQ;AAE3B,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVJ,OAAO,EAAPA;EACF,CAAC;EACDK,IAAI,kBAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,aAAa,EAAE;QAAEC,OAAO,EAAE,EAAE;QAAEC,kBAAkB,EAAE;MAAG,CAAC;MACtDC,eAAe,EAAE,CAAC,CAAC;MACnBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,qBAAG;IACR,IAAI,CAACC,YAAY,EAAE;EACrB,CAAC;EACDC,OAAO,EAAE;IACPD,YAAY,0BAAG;MAAA;MACbZ,GAAE,CACCY,YAAY,EAAC,CACbE,IAAI,CAAC,UAACC,QAAQ,EAAK;QAClB,KAAI,CAACV,SAAQ,GAAIU,QAAQ,CAACX,IAAI;QAC9B,KAAK,IAAIY,KAAK,KAAI,CAACX,SAAS,EAAE;UAC5B,IAAIY,UAAS,GAAI,IAAIC,IAAI,CAAC,KAAI,CAACb,SAAS,CAACW,CAAC,CAAC,CAAC,YAAY,CAAC,CAAC;UAC1DC,UAAU,CAACE,QAAQ,CAACF,UAAU,CAACG,QAAQ,E