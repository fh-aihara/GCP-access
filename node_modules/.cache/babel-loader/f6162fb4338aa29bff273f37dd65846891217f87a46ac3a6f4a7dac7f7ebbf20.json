);\n  },\n  data() {\n    return {\n      campaign_name: null,\n      category: null,\n      keyword: null,\n      abstract: null,\n      detail: null,\n      reword_amount: null,\n      reword_currency: null,\n      distribution_type: null,\n      today: new Date(),\n      start_date_db: new Date(),\n    };\n  },\n  computed: {\n    min_date() {\n      const default_min = new Date(\n        new Date().setDate(this.today.getDate() + 1)\n      );\n      if (default_min >= this.start_date_db) {\n        return this.start_date_db;\n      } else {\n        return default_min;\n      }\n    },\n    filled() {\n      return (\n        this.campaign_name &&\n        this.category &&\n        this.keyword &&\n        this.reword_amount &&\n        this.reword_currency &&\n        this.distribution_type\n      );\n    },\n    btnclass: function () {\n      if (this.filled) {\n        return \"inline-block py-2 px-4 text-xs text-center font-semibold leading-normal text-blue-50 bg-blue-500 hover:bg-blue-600 rounded-lg transition duration-200\";\n      } else {\n        return \"inline-block py-2 px-4 text-xs text-center font-semibold leading-normal text-gray-300 bg-gray-700 hover:bg-gray-800 rounded-lg transition duration-200\";\n      }\n    },\n  },\n  methods: {\n    trashAllData: function () {\n      this.campaign_name = null;\n      this.category = null;\n      this.keyword = null;\n      this.abstract = null;\n      this.detail = null;\n      this.date_from = null;\n      this.date_to = null;\n      this.reword_amount = null;\n      this.reword_currency = null;\n      this.distribution_type = null;\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}